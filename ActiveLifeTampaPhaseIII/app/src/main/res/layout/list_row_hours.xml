        <LinearLayout
            android:id="@+id/hour_two_two_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <com.android.activelife.tampa.customviews.CachetProMediumTextView
                android:id="@+id/hours_four"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
